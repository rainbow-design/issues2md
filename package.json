{
  "name": "issues2md",
  "version": "0.0.1",
  "description": "Export Github Issues (for bloggers) to markdown file",
  "main": "app.js",
  "scripts": {
    "prettier": "prettier --write \"./**/*.{ts,js,md,json}\"",
    "dev": "nodemon -r esm  ./src/app.js",
    "build": "nodemon -r esm ./src/commander.js",
    "predev": "kill-port 3000"
  },
  "bin": "src/app.js",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rainbow-design/issues2md.git"
  },
  "keywords": [
    "github",
    "issues",
    "markdown",
    "export",
    "blog",
    "issue"
  ],
  "author": "yanyue404",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rainbow-design/issues2md/issues"
  },
  "homepage": "https://github.com/rainbow-design/issues2md#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^6.0.0",
    "downloadjs": "^1.4.7",
    "filenamify": "^4.1.0",
    "markdown-js": "^0.0.4",
    "markdown-table": "^1.1.3",
    "request": "^2.88.2",
    "turndown": "^5.0.3",
    "turndown-plugin-gfm": "^1.0.2"
  },
  "devDependencies": {
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "husky": "^3.1.0",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.14",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
